" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"card col-12 col-md-6\" mbr-class=\"{'col-lg-3': cardsAmount == '4',\n                             'col-lg-4': cardsAmount == '3'}\">\n              <div class=\"card-inner\">\n                <div class=\"card-img\">\n                  <a href=\"/search-engine-optimisation\"><span mbr-icon class=\"mbr-iconfont icon-1 fa-google fa\" style=\"color: rgb(236, 137, 160); fill: rgb(236, 137, 160);\"></span></a>\n                </div>\n                <div class=\"card-box\">\n                  <h5 class=\"link align-left mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showLink\" data-app-selector=\".link\"><i style=\"font-weight: normal;\"><a href=\"/search-engine-optimisation\" class=\"text-link text-black\">Search Engine Optimisation</a></i></h5>\n                </div>\n                <div class=\"card-icon\">\n                  <a href=\"/search-engine-optimisation\"><span mbr-icon class=\"mbr-iconfont icon-arrow mobi-mbri-right mobi-mbri\"></span></a>\n                </div>\n              </div>\n            </div>\n            <div class=\"card col-12 col-md-6\" mbr-if=\"cardsAmount > 1\" mbr-class=\"{'col-lg-3': cardsAmount == '4',\n                             'col-lg-4': cardsAmount == '3'}\">\n              <div class=\"card-inner\">\n                <div class=\"card-img\">\n                  <a href=\"/local-seo\"><span mbr-icon class=\"mbr-iconfont icon-1 mobi-mbri-map-pin mobi-mbri\" style=\"color: rgb(236, 137, 160); fill: rgb(236, 137, 160);\"></span></a>\n                </div>\n                <div class=\"card-box\">\n                  <h5 cl