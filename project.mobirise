" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"card col-12 col-md-6\" mbr-class=\"{'col-lg-3': cardsAmount == '4',\n                             'col-lg-4': cardsAmount == '3'}\">\n              <div class=\"card-inner\">\n                <div class=\"card-img\">\n                  <a href=\"/small-business-seo\"><span mbr-icon class=\"mbr-iconfont icon-1 icon54-v1-web-shop1\" style=\"color: rgb(236, 137, 160); fill: rgb(236, 137, 160);\"></span></a>\n                </div>\n                <div class=\"card-box\">\n                  <h5 class=\"link align-left mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showLink\" data-app-selector=\".link\"><i style=\"font-weight: normal;\"><a href=\"/small-business-seo\" class=\"text-link text-black\">Small Business SEO</a></i></h5>\n                </div>\n                <div class=\"card-icon\">\n                  <a href=\"/small-business-seo\"><span mbr-icon class=\"mbr-iconfont icon-arrow mobi-mbri-right mobi-mbri\"></span></a>\n                </div>\n              </div>\n            </div>\n            <div class=\"card col-12 col-md-6\" mbr-if=\"cardsAmount > 1\" mbr-class=\"{'col-lg-3': cardsAmount == '4',\n                             'col-lg-4': cardsAmount == '3'}\">\n              <div class=\"card-inner\">\n                <div class=\"card-img\">\n                  <a href=\"/local-seo\"><span mbr-icon class=\"mbr-iconfont icon-1 mobi-mbri-map-pin mobi-mbri\" style=\"color: rgb(236, 137, 160); fill: rgb(236, 137, 160);\"></span></a>\n                </div>\n                <div class=\"card-box\">\n                  <h5 class=\"link align-left mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showLink\" data-app-selector=\".link\"><a href=\"/local-seo\" class=\"text-link text-black\">Local SEO</a></h5>\n                </div>\n                <div class=\"card-icon\">\n                  <a href=\"/local-seo\"><span mbr-icon class=\"mbr-iconfont icon-arrow mobi-mbri-right mobi-mbri\"></span></a>\n                </div>\n              </div>\n                \n            </div>\n\n            <div class=\"card col-12 col-md-6\" mbr-if=\"cardsAmount > 2\" mbr-class=\"{'col-lg-3': cardsAmount == '4',\n                             'col-lg-4': cardsAmount == '3'}\">\n              <div class=\"card-inner\">\n                <div class=\"card-img\">\n                    <a href=\"/google-ads-ppc\"><span mbr-icon class=\"mbr-iconfont icon-1 sli-graph\" style=\"color: rgb(236, 137, 160); fill: rgb(236, 137, 160);\"></span></a>\n                </div>\n                <div class=\"card-box\">\n                  <h5 class=\"link align-left mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showLink\" data-app-selector=\".link\"><a href=\"/google-ads-ppc\" class=\"text-link text-black\">Google Ads &amp; PPC</a></h5>\n                </div>\n                <div class=\"card-icon\">\n                  <a href=\"/google-ads-ppc\"><span mbr-icon class=\"mbr-iconfont icon-arrow mobi-mbri-right mobi-mbri\"></span></a>\n                </div>\n              </div>\n            </div>\n\n            <div class=\"card col-12 col-md-6\" mbr-if=\"cardsAmount > 3\" mbr-class=\"{'col-lg-3': cardsAmount == '4',\n                             'col-lg-4': cardsAmount == '3'}\">\n              <div class=\"card-inner\">\n                <div class=\"card-img pb-3\">\n                    <span mbr-icon class=\"mbri-desktop mbr-iconfont icon-1\"></span>\n                </div>\n                <div class=\"card-box\">\n                    <h5 class=\"link align-left mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"sh